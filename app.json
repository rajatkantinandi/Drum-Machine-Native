{
  "expo": {
    "name": "Drum Machine",
    "description":
      "A react-native drum machine app based on freeCodeCamp curriculum with 2 sets of pads",
    "slug": "drum-machine",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rajatkn.drummachine"
    },
    "android": {
      "package": "com.rajatkn.drummachine"
    }
  }
}
